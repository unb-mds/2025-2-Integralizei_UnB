    "lint": "eslint ."
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.0.0",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.0"
